    "test-helpers": {
      "root": "libs/test-helpers",
      "sourceRoot": "libs/test-helpers/src",
      "projectType": "library",
      "schematics": {},
      "architect": {
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": [
              "libs/test-helpers/src/**/*.ts",
              "libs/test-helpers/src/**/*.html"
            ]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
