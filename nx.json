{
  "npmScope": "rx-angular",
  "implicitDependencies": {
    "angular.json": "*",
    "package.json": { "dependencies": "*", "devDependencies": "*" },
    "tsconfig.json": "*",
    "tslint.json": "*",
    "nx.json": "*",
    ".eslintrc.json": "*"
  },
  "tasksRunnerOptions": {
    "default": {
      "runner": "@nrwl/workspace/tasks-runners/default",
      "options": { "cacheableOperations": ["build", "lint", "test", "e2e"] }
    }
  },
  "projects": {
    "demos": { "tags": ["type:app"] },
    "demos-e2e": { "tags": [], "implicitDependencies": ["demos"] },
    "test-helpers": { "tags": ["type:util"] },
    "cdk-internals-scheduler": {
      "tags": ["type:lib", "type:cdk", "type:internals"],
      "implicitDependencies": []
    },
    "cdk-zone-less": {
      "tags": ["type:lib", "type:cdk"],
      "implicitDependencies": []
    },
    "cdk-coalescing": {
      "tags": ["type:lib", "type:cdk"],
      "implicitDependencies": []
    },
    "cdk-zone-configurations": {
      "tags": ["type:lib", "type:cdk"],
      "implicitDependencies": []
    },
    "cdk-coercing": {
      "tags": ["type:lib", "type:cdk"],
      "implicitDependencies": []
    },
    "cdk": {
      "tags": ["type:lib", "type:cdk"],
      "implicitDependencies": ["cdk-internals-scheduler", "cdk-coercing", "cdk-zone-less"]
    },
    "state": { "tags": ["type:lib"] },
    "template": {
      "tags": ["type:lib", "type:template"],
      "implicitDependencies": ["cdk-zone-less", "cdk"]
    },
    "docs": { "tags": ["type:app"] },
    "tour-of-heroes": { "tags": ["type:app"] },
    "tour-of-heroes-ngxs": { "tags": ["type:app"] },
    "vanilla-case-studies": { "tags": [] },
    "vanilla-case-studies-e2e": {
      "tags": [],
      "implicitDependencies": ["vanilla-case-studies"]
    },
    "ssr": { "tags": [] },
    "ssr-e2e": { "tags": [], "implicitDependencies": ["ssr"] }
  },
  "affected": { "defaultBase": "master" }
}
